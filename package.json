{
    "name": "spareparts",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "build-docker": "next build --no-lint",
        "lint": "tsc --noEmit && next lint",
        "lint:fix": "next lint --fix",
        "test": "env-cmd -f .env.test vitest",
        "test:unit": "env-cmd -f .env.test vitest run tests/boat-configuration/unit",
        "test:integration": "env-cmd -f .env.test vitest run tests/boat-configuration/integration",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:headed": "playwright test --headed",
        "test:coverage": "env-cmd -f .env.test vitest run --coverage",
        "test:watch": "env-cmd -f .env.test vitest",
        "test:ci": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
        "coverage": "env-cmd -f .env.test vitest run --coverage",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "db:connect": "env-cmd -f .env tsx scripts/testConnection.ts",
        "deploy:db:connect": "tsx scripts/testConnection.ts",
        "docs:pdf": "node scripts/generate-pdf-documentation-v2.js"
    },
